    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "devDependencies": {
